conda remove --name qgisenv --all
conda create -n qgisenv python=3.9
conda activate qgisenv
conda install pandas pyproj numpy scikit-learn matplotlib shapely scipy geopandas
pip install trimesh nrel-pysam
conda install -c conda-forge qgis
conda install python==3.11

